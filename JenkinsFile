pipeline{
   agent any
   
   stages{
      stage('Compile Stage'){
         steps{
            withMaven(maven : 'Maven_3.5.4'){
               sh 'mvn clean package'
               }
            }
         }
       stage('testing Stage'){
         steps{
            withMaven(maven : 'Maven_3.5.4'){
               sh 'mvn test'
               }
            }
         }
       stage('Deployment Stage'){
         steps{
            withMaven(maven : 'Maven_3.5.4'){
               sh 'mvn deploy'
               }
            }
         }
         }
}
